     console.log(num);\n      if (!num in nums) {\n        console.log(\"REJECTION\", num, nums);\n        return \"display: none\";\n      }\n      var placements = nums[num + \"\"];\n      console.log(nums);\n      if (!placements) {\n        console.log(\"Placements is undefined\", num + \"\", placements);\n        return \"display: none\";\n      }\n      if (num >= placements.length) {\n        console.log(\"REJECTION\", num);\n        return \"display: none;\";\n      }\n      var pos = this.toPosition(placements[num]);\n      var ret = `\n                position: absolute;\n                top: ${pos.y}mm;\n                left: ${pos.x}mm;\n            `;\n      return ret;\n    },\n    getBox() {\n      var ret = `\n                position: absolute;\n                top: ${this.y}mm;\n                left: ${this.x}mm;\n                width: ${this.width}mm;\n                height: ${this.height}mm;\n            `;\n      console.log(ret);\n      return ret;\n    }\n  },\n  mounted() {},\n  watch: {\n    output: function (old, newVal) {\n      if (newVal) {\n        this.download();\n      }\n    }\n  }\n};","map":{"version":3,"names":["nums","x","y","name","props","data","methods","pxToMm","px","div","document","createElement","style","display","height","body","appendChild","convert","offsetHeight","parentNode","removeChild","mmToPx","mm","toPosition","percent","xPos","Number","width","imagewidth","yPos","getNumberStyle","num","console","log","placements","length","pos","ret","getBox","mounted","watch","output","old","newVal","download"],"sources":["/git/card-gen/card-gen/src/components/Numbers.vue"],"sourcesContent":["<script></script>\n<template>\n    <div class=\"surround\" :style=\"getBox()\">\n        <img v-for=\"i in 11\" v-bind:key=\"i\" :src=\"suitimage\" :width=\"mmToPx(this.imagewidth)\" :height=\"mmToPx(this.imagewidth)\" class=\"suit\" :style=\"getNumberStyle(i)\"/>\n    </div>\n</template>\n\n<style scoped>\n.surround {\n    /* border: 1px solid black; */\n    position: absolute;\n}\n</style>\n<script>\n\n// These are all done by percents\n// They will be translated by the card directly\nvar nums = {\n    '6': [\n        {x: 0, y: 0},\n        {x: 1, y: 0},\n        {x: 0, y: 0.5},\n        {x: 1, y: 0.5},\n        {x: 0, y: 1},\n        {x: 1, y: 1},\n    ],\n    '7': [\n        {x: 0, y: 0},\n        {x: 1, y: 0},\n        {x: 0, y: 0.5},\n        {x: 1, y: 0.5},\n        {x: 0, y: 1},\n        {x: 1, y: 1},\n        {x: 0.5, y: 0.25},\n    ],\n    '10': [\n        {x: 0, y: 0},\n        {x: 0, y: 0.33},\n        {x: 0, y: 0.66},\n        {x: 0, y: 1},\n\n        {x: 1, y: 0},\n        {x: 1, y: 0.33},\n        {x: 1, y: 0.66},\n        {x: 1, y: 1},\n\n        {x: 0.5, y: 0.33/2},\n        {x: 0.5, y: 1-0.3/2},\n    ],\n}\n\nexport default {\n    name: 'Numbers',\n    props: [\n        'suitimage',\n        'number',\n        \"x\",\n        \"y\",\n        \"width\",\n        \"height\",\n        \"imagewidth\"\n    ],\n    data() {\n        return {}\n    },\n    methods: {\n        pxToMm(px) {\n            var div = document.createElement('div');\n            div.style.display = 'block';\n            div.style.height = '100mm';\n            document.body.appendChild(div);\n            var convert = 100 / div.offsetHeight;\n            convert = px * convert;\n            div.parentNode.removeChild(div);\n            return convert;\n        },\n        mmToPx(mm) {\n            var div = document.createElement('div');\n            div.style.display = 'block';\n            div.style.height = '100mm';\n            document.body.appendChild(div);\n            var convert = div.offsetHeight / 100;\n            convert = mm * convert;\n            div.parentNode.removeChild(div);\n            return convert;\n        },\n        toPosition(percent) {\n            // Find the x distance\n            var xPos = Number(this.width) * percent.x - Number(this.imagewidth)/2;\n            var yPos = Number(this.height) * percent.y - Number(this.imagewidth)/2;\n            return {\n                x: xPos,\n                y: yPos,\n            }\n        },\n        getNumberStyle(num) {\n            console.log(num)\n            if (!num in nums) {\n                console.log(\"REJECTION\", num, nums);\n                return \"display: none\";\n            }\n            var placements = nums[num + \"\"];\n            console.log(nums);\n            if (!placements) {\n                console.log(\"Placements is undefined\", num + \"\", placements);\n                return \"display: none\";\n            }\n            if (num >= placements.length) {\n                console.log(\"REJECTION\", num);\n                return \"display: none;\";\n            }\n            var pos = this.toPosition(placements[num]);\n            var ret = `\n                position: absolute;\n                top: ${pos.y}mm;\n                left: ${pos.x}mm;\n            `;\n            return ret;\n        },\n        getBox() {\n            var ret = `\n                position: absolute;\n                top: ${this.y}mm;\n                left: ${this.x}mm;\n                width: ${this.width}mm;\n                height: ${this.height}mm;\n            `;\n            console.log(ret);\n            return ret;\n        }\n    },\n    mounted() {\n    },\n    watch: {\n        output: function (old, newVal) {\n            if (newVal) {\n                this.download()\n            }\n        }\n    },\n}\n</script>\n"],"mappings":"AAeA;AACA;AACA,IAAIA,IAAG,GAAI;EACP,GAAG,EAAE,CACD;IAACC,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAG,CAAC,EACd;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAG,CAAC,EACd;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,CACf;EACD,GAAG,EAAE,CACD;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CA