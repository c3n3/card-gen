 this.toPosition(placements[num]);\n      var ret = `\n                position: absolute;\n                top: ${pos.y}mm;\n                left: ${pos.x}mm;\n            `;\n      return ret;\n    },\n    getBox() {\n      var ret = `\n                position: absolute;\n                top: ${this.y}mm;\n                left: ${this.x}mm;\n                width: ${this.width}mm;\n                height: ${this.height}mm;\n            `;\n      console.log(ret);\n      return ret;\n    }\n  },\n  mounted() {},\n  watch: {\n    output: function (old, newVal) {\n      if (newVal) {\n        this.download();\n      }\n    }\n  }\n};","map":{"version":3,"names":["nums","x","y","name","props","data","methods","pxToMm","px","div","document","createElement","style","display","height","body","appendChild","convert","offsetHeight","parentNode","removeChild","mmToPx","mm","toPosition","percent","xPos","Number","width","yPos","console","log","imagewidth","getNumberStyle","num","placements","length","pos","ret","getBox","mounted","watch","output","old","newVal","download"],"sources":["/git/card-gen/card-gen/src/components/Numbers.vue"],"sourcesContent":["<script></script>\n<template>\n    <div class=\"surround\" :style=\"getBox()\">\n        <img v-for=\"i in 10\" v-bind:key=\"i\" :src=\"suitimage\" :width=\"mmToPx(this.imagewidth)\" :height=\"mmToPx(this.imagewidth)\" class=\"suit\" :style=\"getNumberStyle(i-1)\"/>\n    </div>\n</template>\n\n<style scoped>\n.surround {\n    border: 1px solid black;\n    position: absolute;\n}\n</style>\n<script>\n\n// These are all done by percents\n// They will be translated by the card directly\nvar nums = {\n    '6': [\n        {x: 0, y: 0},\n        {x: 1, y: 0},\n        {x: 0, y: 0.5},\n        {x: 1, y: 0.5},\n        {x: 0, y: 1},\n        {x: 1, y: 1},\n    ],\n    '7': [\n        {x: 0, y: 0},\n        {x: 1, y: 0},\n        {x: 0, y: 0.5},\n        {x: 1, y: 0.5},\n        {x: 0, y: 1},\n        {x: 1, y: 1},\n        {x: 0.5, 