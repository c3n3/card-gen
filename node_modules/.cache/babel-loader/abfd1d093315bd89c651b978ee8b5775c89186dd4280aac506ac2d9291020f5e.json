     var xPos = this.width * percent + this.x - this.imagewidth;\n            var yPos = this.height * percent + this.y - this.imagewidth;\n            return {\n                x: xPos,\n                y: yPos,\n            }\n        },\n        getNumberStyle(num) {\n            var placements = nums['6'];\n            var pos = this.toPosition(placements[num]);\n            console.log(pos)\n            var ret = `\n                position: absolute;\n                top: ${pos.y}mm;\n                left: ${pos.x}mm;\n            `;\n            return ret;\n        },\n    },\n    mounted() {\n    },\n    watch: {\n        output: function (old, newVal) {\n            if (newVal) {\n                this.download()\n            }\n        }\n    },\n}\n</script>\n"],"mappings":"AAYA;AACA;AACA,IAAIA,IAAG,GAAI;EACP,GAAG,EAAE,CACD;IAACC,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAG,CAAC,EACd;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAG,CAAC,EACd;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC,EACZ;IAACD,CAAC,EAAE,CAAC;IAAEC,CAAC,EAAE;EAAC,CAAC;AAEpB;AAEA,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,KAAK,EAAE,CACH,WAAW,EACX,QAAQ,EACR,GAAG,EACH,GAAG,EACH,OAAO,EACP,QAAQ,EACR,YAAW,CACd;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO,CAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACLC,MAAMA,CAACC,EAAE,EAAE;MACP,IAAIC,GAAE,GAAIC,QAAQ,CAACC,aAAa,CAAC,KAAK,CAAC;MACvCF,GAAG,CAACG,KAAK,CAACC,OAAM,GAAI,OAAO;MAC3BJ,GAAG,CAACG,KAAK,CAACE,MAAK,GAAI,OAAO;MAC1BJ,QAAQ,CAACK,IAAI,CAACC,WAAW,CAACP,GAAG,CAAC;MAC9B,IAAIQ,OAAM,GAAI,GAAE,GAAIR,GAAG,CAACS,YAAY;MACpCD,OAAM,GAAIT,EAAC,GAAIS,OAAO;MACtBR,GAAG,CAACU,UAAU,CAACC,WAAW,CAACX,GAAG,CAAC;MAC/B,OAAOQ,OAAO;IAClB,CAAC;IACDI,MAAMA,CAACC,EAAE,EAAE;MACP,IAAIb,GAAE,GAAIC,QAAQ,CAACC,aAAa,CAAC,KAAK,CAAC;MACvCF,GAAG,CAACG,KAAK,CAACC,OAAM,GAAI,OAAO;MAC3BJ,GAAG,CAACG,KAAK,CAACE,MAAK,GAAI,OAAO;MAC1BJ,QAAQ,CAACK,IAAI,CAACC,WAAW,CAACP,GAAG,CAAC;MAC9B,IAAIQ,OAAM,GAAIR,GAAG,CAACS,YAAW,GAAI,GAAG;MACpCD,